       <STATUS>R</STATUS>
       <LENGTH>041</LENGTH>
       <TEXT>Eliminar entrada de produto</TEXT>
      </SOTR_TEXT>
     </ENTRIES>
    </item>
   </SOTR>
   <SOTR_USE>
