<mvc:View controllerName="com.vistex.zagriplan.controller.Detail3" xmlns="sap.ui.table" xmlns:f="sap.ui.layout.form"                                                                                                                                           
	xmlns:dnd="sap.ui.core.dnd" xmlns:mvc="sap.ui.core.mvc" xmlns:c="sap.ui.core" xmlns:m="sap.m" height="100%" xmlns:html="http://www.w3.org/1999/xhtml">                                                                                                        
	<m:Page title="{i18n>detail3Title}" showNavButton="true" navButtonPress="onNavBack" showHeader="true" enableScrolling="false"                                                                                                                                 
		class="sapUiContentPadding">                                                                                                                                                                                                                                 
		<m:headerContent>                                                                                                                                                                                                                                            
			<m:Button id="idFull" icon="sap-icon://full-screen" tooltip="Share" press="onFullScreen"/>                                                                                                                                                                  
			<m:Button id="idExit" icon="sap-icon://exit-full-screen" tooltip="Share" visible="false" press="onFullScreen" />			                                                                                                                                         
		</m:headerContent>		                                                                                                                                                                                                                                         
		<m:content>                                                                                                                                                                                                                                                  
			<m:Panel id="idPanelHeader" visible="true" expanded="false" expandable="false" headerText="">                                                                                                                                                               
				<f:Form id="FormDisplay354" editable="false">                                                                                                                                                                                                              
					<f:layout>                                                                                                                                                                                                                                                
						<f:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4"                                                                                                             
							emptySpanM="4" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false"/>                                                                                                                                                 
					</f:layout>                                                                                                                                                                                                                                               
					<f:formContainers>                                                                                                                                                                                                                                        
						<f:FormContainer>                                                                                                                                                                                                                                        
							<f:formElements>                                                                                                                                                                                                                                        
								<f:FormElement label="{i18n_PT>titleMaster}">                                                                                                                                                                                                          
									<f:fields>                                                                                                                                                                                                                                            
										<m:Text id="idArea"/>                                                                                                                                                                                                                                
									</f:fields>                                                                                                                                                                                                                                           
								</f:FormElement>                                                                                                                                                                                                                                       
								<f:FormElement label="Centro">                                                                                                                                                                                                                         
									<f:fields>                                                                                                                                                                                                                                            
										<m:Text id="idCentro"/>                                                                                                                                                                                                                              
									</f:fields>                                                                                                                                                                                                                                           
								</f:FormElement>                                                                                                                                                                                                                                       
								<f:FormElement label="Safra">                                                                                                                                                                                                                          
									<f:fields>                                                                                                                                                                                                                                            
										<m:Text id="idSafra"/>                                                                                                                                                                                                                               
									</f:fields>                                                                                                                                                                                                                                           
								</f:FormElement>                                                                                                                                                                                                                                       
							</f:formElements>                                                                                                                                                                                                                                       
						</f:FormContainer>                                                                                                                                                                                                                                       
						<f:FormContainer>                                                                                                                                                                                                                                        
							<f:formElements>                                                                                                                                                                                                                                        
								<f:FormElement label="Processo">                                                                                                                                                                                                                       
									<f:fields>                                                                                                                                                                                                                                            
										<m:Text id="idProc"/>                                                                                                                                                                                                                                
									</f:fields>                                                                                                                                                                                                                                           
								</f:FormElement>                                                                                                                                                                                                                                       
								<f:FormElement label="Tarefa">                                                                                                                                                                                                                         
									<f:fields>                                                                                                                                                                                                                                            
										<m:Text id="idTask"/>                                                                                                                                                                                                                                
									</f:fields>                                                                                                                                                                                                                                           
								</f:FormElement>                                                                                                                                                                                                                                       
								<f:FormElement visible="false" label="Data">                                                                                                                                                                                                           
									<f:fields>                                                                                                                                                                                                                                            
										<m:Text id="idData"/>                                                                                                                                                                                                                                
									</f:fields>                                                                                                                                                                                                                                           
								</f:FormElement>                                                                                                                                                                                                                                       
							</f:formElements>                                                                                                                                                                                                                                       
						</f:FormContainer>                                                                                                                                                                                                                                       
					</f:formContainers>                                                                                                                                                                                                                                       
				</f:Form>                                                                                                                                                                                                                                                  
				<m:SegmentedButton selectedKey="{gm>/detail3/sMapTable}" selectionChange="onChangeMapTable">                                                                                                                                                               
					<m:items>                                                                                                                                                                                                                                                 
						<m:SegmentedButtonItem icon="sap-icon://table-view" key="table" />                                                                                                                                                                                       
						<m:SegmentedButtonItem icon="sap-icon://map-2" key="map"/>                                                                                                                                                                                               
					</m:items>                                                                                                                                                                                                                                                
				</m:SegmentedButton>                                                                                                                                                                                                                                       
			</m:Panel>                                                                                                                                                                                                                                                  
			<m:HBox renderType="Bare">                                                                                                                                                                                                                                  
			    <html:div id="map" style="padding: 0;margin: 0;height: 100%;width: 100%; position:relative;">                                                                                                                                                           
			    	<html:img id="loadingImg" src="images/loading.gif" style="position:absolute; left:30%; top:35%; z-index:100; width:200px; height:160px" />                                                                                                             
			    </html:div>                                                                                                                                                                                                                                             
				<Table id="table1" width="100%" selectionMode="Multi" rows="{modelTabDisp>/results}" visible="{= ${gm>/detail3/sMapTable} === 'table' }">                                                                                                                  
					<extension>                                                                                                                                                                                                                                               
						<m:OverflowToolbar>                                                                                                                                                                                                                                      
							<m:Title text="{i18n>talhoesdisponives}"/>                                                                                                                                                                                                              
						</m:OverflowToolbar>                                                                                                                                                                                                                                     
					</extension>                                                                                                                                                                                                                                              
					<columns>                                                                                                                                                                                                                                                 
						<Column width="auto">                                                                                                                                                                                                                                    
							<m:Text text="Terreno"/>                                                                                                                                                                                                                                
							<template>                                                                                                                                                                                                                                              
								<m:Text text="{modelTabDisp>Talhao}" wrapping="false"/>                                                                                                                                                                                                
							</template>                                                                                                                                                                                                                                             
						</Column>                                                                                                                                                                                                                                                
						<Column width="auto">                                                                                                                                                                                                                                    
							<m:Text text="Cultura"/>                                                                                                                                                                                                                                
							<template>                                                                                                                                                                                                                                              
								<m:Text text="{modelTabDisp>Cmnum}" wrapping="false"/>                                                                                                                                                                                                 
							</template>                                                                                                                                                                                                                                             
						</Column>                                                                                                                                                                                                                                                
						<Column width="60px">                                                                                                                                                                                                                                    
							<m:Text text="{i18n>area}"/>                                                                                                                                                                                                                            
							<template>                                                                                                                                                                                                                                              
								<m:Text text="{modelTabDisp>Aarea}"/>                                                                                                                                                                                                                  
							</template>                                                                                                                                                                                                                                             
						</Column>                                                                                                                                                                                                                                                
						<Column visible="false" width="auto">                                                                                                                                                                                                                    
							<m:Text text="Data"/>                                                                                                                                                                                                                                   
							<template>                                                                                                                                                                                                                                              
								<m:DatePicker value="{modelSelect>Data}" displayFormat="dd.MM.yyyy" class="sapUiSmallMarginBottom"/>                                                                                                                                                   
							</template>                                                                                                                                                                                                                                             
						</Column>                                                                                                                                                                                                                                                
					</columns>                                                                                                                                                                                                                                                
				</Table>&nbsp;                                                                                                                                                                                                                                             
				<m:VBox justifyContent="Center" class="sapUiTinyMarginBeginEnd" >                                                                                                                                                                                          
					<m:Button class="sapUiTinyMarginBottom" icon="sap-icon://navigation-right-arrow" tooltip="Move to selected" press="moveToTable2"/>                                                                                                                        
					<m:Button icon="sap-icon://navigation-left-arrow" tooltip="Move to available" press="moveToTable1"/>                                                                                                                                                      
				</m:VBox>                                                                                                                                                                                                                                                  
				<Table id="table2" width="100%" selectionMode="Multi" rows="{modelSelect>/results}" rowSelectionChange="onTableRowSelect" noData="Selecionar terrenos">                                                                                                    
					<extension>                                                                                                                                                                                                                                               
						<m:OverflowToolbar>                                                                                                                                                                                                                                      
							<m:Title text="{i18n>d3_t2_Title}"/>                                                                                                                                                                                                                    
							<m:Button icon = "sap-icon://delete" press ="onDelete"/>                                                                                                                                                                                                
							<m:ToolbarSpacer/>                                                                                                                                                                                                                                      
						<!--	<m:Button icon="sap-icon://navigation-up-arrow" tooltip="Move up" press="moveUp"/>                                                                                                                                                                  
							<m:Button icon="sap-icon://navigation-down-arrow" tooltip="Move down" press="moveDown"/>-->                                                                                                                                                             
						</m:OverflowToolbar>                                                                                                                                                                                                                                     
					</extension>                                                                                                                                                                                                                                              
					<columns>                                                                                                                                                                                                                                                 
						<Column filtered="false"  sortProperty="Talhao" width="10rem">                                                                                                                                                                                           
							<m:Text text="Terreno"/>                                                                                                                                                                                                                                
							<template>                                                                                                                                                                                                                                              
								<m:Text text="{Talhao}" wrapping="false"/>                                                                                                                                                                                                             
							</template>                                                                                                                                                                                                                                             
						</Column>                                                                                                                                                                                                                                                
						<Column width = "8rem">                                                                                                                                                                                                                                  
							<m:Text text="Cultura"/>                                                                                                                                                                                                                                
							<template>                                                                                                                                                                                                                                              
								<m:Text text="{Cmnum}" wrapping="false"/>                                                                                                                                                                                                              
							</template>                                                                                                                                                                                                                                             
						</Column>                                                                                                                                                                                                                                                
						<Column width="8rem">                                                                                                                                                                                                                                    
							<m:Text text="{i18n>area}"/>                                                                                                                                                                                                                            
							<template>                                                                                                                                                                                                                                              
								<m:Text text="{Aarea}"/>                                                                                                                                                                                                                               
							</template>                                                                                                                                                                                                                                             
						</Column>                                                                                                                                                                                                                                                
						<Column width="8rem" sortProperty="Data">                                                                                                                                                                                                                
							<m:Text text="Data"/>                                                                                                                                                                                                                                   
							<template>                                                                                                                                                                                                                                              
								<m:DatePicker value="{Data}" displayFormat="dd.MM.yyyy" class="tableDate"/>                                                                                                                                                                            
							</template>                                                                                                                                                                                                                                             
						</Column>                                                                                                                                                                                                                                                
						<Column width="8rem">                                                                                                                                                                                                                                    
							<m:Text text="Ordem tarefa"/>                                                                                                                                                                                                                           
							<template>                                                                                                                                                                                                                                              
								<m:Text text="{Aufnr}"/>                                                                                                                                                                                                                               
							</template>                                                                                                                                                                                                                                             
						</Column>					                                                                                                                                                                                                                                           
						<Column width="8rem">                                                                                                                                                                                                                                    
							<m:Text text="Receita"/>                                                                                                                                                                                                                                
							<template>                                                                                                                                                                                                                                              
								<m:Text text="{Rcnum}"/>                                                                                                                                                                                                                               
							</template>                                                                                                                                                                                                                                             
						</Column>                                                                                                                                                                                                                                                
							<Column width="8rem">                                                                                                                                                                                                                                   
							<m:Text text="Tec.Encerrado"/>                                                                                                                                                                                                                          
							<template>                                                                                                                                                                                                                                              
								<m:CheckBox editable="false" selected="{= ${Status} === 'X' }" />                                                                                                                                                                                      
							</template>                                                                                                                                                                                                                                             
						</Column>	                                                                                                                                                                                                                                               
					</columns>                                                                                                                                                                                                                                                
				</Table>                                                                                                                                                                                                                                                   
			</m:HBox>                                                                                                                                                                                                                                                   
		</m:content>                                                                                                                                                                                                                                                 
		<m:footer>                                                                                                                                                                                                                                                   
			<m:Bar>                                                                                                                                                                                                                                                     
				<m:contentRight>                                                                                                                                                                                                                                           
					<m:Button icon="sap-icon://activity-items" text="Encerrar Tec." type="Accept" press="onTechTaskorder"/>                                                                                                                                                   
					<m:Button icon="sap-icon://save" text="Salvar" type="Accept" press="onSaveProg"/>                                                                                                                                                                         
					<m:Button icon="sap-icon://activity-2" text="Ordem tarefa" type="Default" press="onCreateTaskConfirm"/>                                                                                                                                                   
					<m:Button icon="sap-icon://activity-items" text="Receitas" type="Default" press="openReceitas"/>                                                                                                                                                          
				</m:contentRight>                                                                                                                                                                                                                                          
			</m:Bar>                                                                                                                                                                                                                                                    
		</m:footer>                                                                                                                                                                                                                                                  
	</m:Page>                                                                                                                                                                                                                                                     
</mvc:View>                                                                                                                                                                                                                                                    