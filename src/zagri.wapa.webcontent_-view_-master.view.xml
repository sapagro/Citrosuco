<mvc:View controllerName="com.vistex.zagriplan.controller.Master"                                                                                                                                                                                              
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">                                                                                                                                                                                                
	<Page title="{i18n_PT>titleMaster}">                                                                                                                                                                                                                          
		<content>                                                                                                                                                                                                                                                    
			<Toolbar>                                                                                                                                                                                                                                                   
				<SearchField liveChange="onSearch" />                                                                                                                                                                                                                      
				<Button icon="sap-icon://add-filter" press="onShowFilters"/>                                                                                                                                                                                               
			</Toolbar>                                                                                                                                                                                                                                                  
			<List id="idList" items="{masterModel>/results}" busyIndicatorDelay="0">                                                                                                                                                                                    
				<items>                                                                                                                                                                                                                                                    
					<ObjectListItem title="{masterModel>Acdes}" type="Active"&nbsp;&nbsp;&nbsp;                                                                                                                                                                               
						press="onListItemPress" number="{masterModel>Arabs}" numberUnit="{masterModel>Unabs}">                                                                                                                                                                   
						<attributes>                                                                                                                                                                                                                                             
							<ObjectAttribute title="Centro" text="{masterModel>Werks}" />                                                                                                                                                                                           
							<ObjectAttribute title="Safra" text="{masterModel>Ajahr}" />                                                                                                                                                                                            
							<!--<ObjectAttribute text="{masterModel>Werks}" />							-->                                                                                                                                                                                            
							<!--<ObjectAttribute text="{masterModel>Arabs}" />-->                                                                                                                                                                                                   
						</attributes>                                                                                                                                                                                                                                            
					</ObjectListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
			</List>                                                                                                                                                                                                                                                     
		</content>                                                                                                                                                                                                                                                   
		<footer>                                                                                                                                                                                                                                                     
			<Bar>                                                                                                                                                                                                                                                       
			    <contentRight>                                                                                                                                                                                                                                          
				    <Button visible="false" icon="sap-icon://add-filter" press="onShowFilters"/>			&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                        
			    </contentRight>                                                                                                                                                                                                                                         
			</Bar>                                                                                                                                                                                                                                                      
		</footer>                                                                                                                                                                                                                                                    
	</Page>                                                                                                                                                                                                                                                       
</mvc:View>                                                                                                                                                                                                                                                    