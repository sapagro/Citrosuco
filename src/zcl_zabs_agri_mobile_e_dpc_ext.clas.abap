    SELECT a~clint
    INTO TABLE @lt_gclint
    FROM klah AS a
      INNER JOIN /agri/glagha AS b
    ON ( b~class = a~class )
    WHERE a~klart = @lc_klart "zcl_lin_abs_odata_abap=>c_agtyp-measurement_point
