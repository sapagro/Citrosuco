			this.oModel = this.getOwnerComponent().getModel();                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			this.oRouter.getRoute("DetailDetail").attachPatternMatched(this.onRouteMatched(this));		&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                            
                                                                                                                                                                                                                                                               
		},                                                                                                                                                                                                                                                           
